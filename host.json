{
  "version": "2.0",
  "extensions": {
    "durableTask": {
      "hubName": "newTestHub222",
      "storageProvider": {
        "maxQueuePollingInterval": "00:00:01"
      },
      "maxConcurrentActivityFunctions": 40,
      "maxConcurrentOrchestratorFunctions": 4
    }
  },
    "logging": {
        "applicationInsights": {
            "samplingSettings": {
                "isEnabled": true,
                "excludedTypes": "Request"
            },
            "enableLiveMetricsFilters": true
        }
    }
}